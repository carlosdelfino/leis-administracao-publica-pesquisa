{
   "title": "A Administração Pública, A Pesquisa Ciêntifica e Nossa Sociedade",
   "language": "pt",
   "plugins": [
       "richquotes",
       "disqus",
       "extend-default-theme",
       "search",
       "image-captions",
       "katex",
       "indexes",
       "customtheme",
       "theme-api",
       "theme-default",
       "bulk-redirect",
       "zingchart",
       "jsfiddle",
       "ace",
       "ga",
       "wabtheme",
       "page-toc"
   ],
   "pluginsConfig": {
       "disqus": {
           "shortName": "aadministracaopublicaapesquisacientificaenossasociedade",
           "useIdentifier": true
       },
       "page-toc": {
           "selector": " .markdown-section h2, .markdown-section h3, .markdown-section h4",
           "position": "top"
       },
       "layout": {
           "headerPath": "assets/layouts/header.html",
           "footerPath": "assets/layouts/footer.html"
       },
       "richquotes": {
           "default": true
       },
       "bulk-redirect": {
           "basepath": "/",
           "redirectsFile": "redirects.json"
       },
       "ga": {
           "token": "UA-21710445-1",
           "configuration": "auto"
       },
       "sharing": {
           "facebook": true,
           "twitter": true,
           "google": true,
           "weibo": false,
           "instapaper": true,
           "vk": false,
           "all": [
               "facebook",
               "google",
               "twitter",
               "weibo",
               "instapaper",
               "vk"
           ]
       },
       "image-captions": {
           "caption": "Imagem _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_ - _CAPTION_",
           "list_caption": "Lista de Imagens _BOOK_IMAGE_NUMBER_: _CAPTION_",
           "variable_name": "picturesList",
           "Images": {
               "1.1.1": {
                  "skip": "true"
               }
           }
       }
   },
   "structure": {
       "readme": "readme.md",
       "glossary": "glossario.md",
       "summary": "SUMMARY.md"
   },
   "variables": {
       "Version": "0.1"
   },
   "styles": { 
     "website": "assets/styles/website.css", "ebook": "assets/styles/ebook.css", "pdf": "assets/styles/pdf.css", "mobi": "assets/styles/ebook.css", "epub": "assets/styles/ebook.css" },
      "links": {
       "sidebar": {
           "Site": "http://carlosdelfino.eti.br",
         "Facebook": "https://www.facebook.com/ProfessorCarlosDelfino/",
         "Aulas Particulares": "mailto:consultoria@carlosdelfino.eti.br?subject='Gostaria de Informações sobre Aula Particular'&body='Meu nome é<br/>Gostaria de obter informações, gostaria de ter aulas em grupos ou individuais.'"
       }
   }

}